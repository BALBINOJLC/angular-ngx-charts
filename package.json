    "@angular-eslint/eslint-plugin-template": "^17.2.0",
    "@angular/cli": "16.0.0",
    "@angular/compiler-cli": "16.0.0",
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@nguniversal/builders": "^15.2.1",
    "@tailwindcss/line-clamp": "0.4.2",
