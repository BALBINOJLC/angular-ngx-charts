{
  "name": "sicrux-teams-agile",
  "version": "0.1.67",
  "description": "Sicrux Teams Agile",
  "author": "https://sicrux.tech",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ng": "ng",
    "prebuild": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ng serve --port 4010",
    "start:o": "ng serve --port 4010 -o",
    "start:hot": "ng serve --port 4010 --hmr",
    "build-no-ssr": "ng build",
    "build:production": "ng build --configuration production",
    "build:staging": "ng build --configuration staging",
    "watch": "ng build --watch --configuration development",
    "test": "ng build",
    "prepare": "husky install",
    "deploy:production": "ng deploy",
    "deploy:staging": "git checkout staging && git merge develop && yarn build:staging && yarn release && git push --follow-tags origin staging && git push && git checkout develop && git merge staging && git push",
    "deploy:main": "git checkout main && git merge develop && yarn build && yarn release && git push --follow-tags origin main && git push && git checkout develop && git merge main && git push",
    "release": "standard-version",
    "dev:ssr": "ng run fuse:serve-ssr",
    "serve:ssr": "node dist/fuse/server/main.js",
    "build:ssr": "ng build && ng run fuse:server",
    "prerender": "ng run fuse:prerender",
    "delete:dist": "rm -rf dist",
    "build": "ng build",
    "copy:hosting": "cp -r ./dist/fuse/* ./public && rm -f ./public/index.html",
    "build:functions": "yarn --prefix functions build"
  },
  "dependencies": {
    "@angular-material-components/datetime-picker": "^8.0.0",
